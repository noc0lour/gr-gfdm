<?xml version="1.0"?>
<block>
  <name>channel_estimator_cc</name>
  <key>gfdm_channel_estimator_cc</key>
  <category>[gfdm]</category>
  <import>import gfdm</import>
  <make>gfdm.channel_estimator_cc($timeslots, $subcarriers, $cp_len, $preamble_data, $preamble_f_taps, $gfdm_block_tag_key)</make>
  <param>
    <name># timeslots</name>
    <key>timeslots</key>
    <value>4</value>
    <type>int</type>
  </param>
  <param>
    <name># subcarriers</name>
    <key>subcarriers</key>
    <value>16</value>
    <type>int</type>
  </param>
  <param>
    <name>cyclic prefix length</name>
    <key>cp_len</key>
    <value>5</value>
    <type>int</type>
  </param>
  <param>
    <name>Known preamble data</name>
    <key>preamble_data</key>
    <type>raw</type>
  </param>
  <param>
    <name>Known preamble frequency taps</name>
    <key>preamble_f_taps</key>
    <type>raw</type>
  </param>
  <param>
    <name>GFDM block tag key</name>
    <key>gfdm_block_tag_key</key>
    <value>"gfdm_block"</value>
    <type>string</type>
    
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
